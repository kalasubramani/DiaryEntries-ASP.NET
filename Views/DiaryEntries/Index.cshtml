@model List<DiaryEntry>

<div class="table-responsive">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<th>Title</th>
				<th>Content</th>
				<th>Date</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			@* get each record in Model and map the ppts to each <td> *@
			@foreach(var entry in Model){
				<tr>
					<td>@entry.Title</td>
					<td>@entry.Content</td>
					<td>@entry.Created</td>
					<td>Edit and Delete!</td>
				</tr>
			}
		</tbody>
	</table>

</div> 